% Add a heart block in this map

@ $Script_MakeEntities
{
    0:  If  *GB_StoryProgress  <  .Story:Epilogue % 60
   10:  	Call  MakeEntity    ( .Entity:SavePoint ~Vec4d:Entity80249140 80000000 )
            Call  MakeEntity    ( .Entity:HealingBlock ~Vec4d:Entity_HeartBlock 80000000 )
   34:  EndIf
   3C:  Return
   44:  End
}